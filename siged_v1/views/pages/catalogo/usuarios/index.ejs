<div id="index" v-cloak>
    <div class="row">
      <div class="col-md-12 text-left">
        <button type="button" name="btn btn-primary btn-md">Nuevo Usuario</button>
      </div>
    </div>
    <div class="x_content">
      <div class="table-responsive">

        <table id="dtUsuarios" class="table table-striped jambo_table bulk_action" style="width:100%">
          <thead>
            <tr>
              <th>Estado</th>
              <th>Nombre completo</th>
              <th>E-mail</th>
              <th class="text-center">Administrador</th>
              <th class="text-center">Google ID</th>
              <th v-if="me.isSuperAdmin" class="text-center">Acci√≥n</th>
              </tr>
            </thead>
            <tbody>
              <tr v-for="response in response">
                <td v-if="response.estado"><img src="data:image/png;base64,"/> 
                </td>
                <td v-else>
                  <img src="data:image/png;base64,">
                </td>
                <td>{{ response.fullName }}</td>
                <td>{{ response.emailAddress }}</td>
                <td class="text-center" v-if="response.isSuperAdmin">Si</td>
                <td class="text-center" v-else>No</td>
                <td class="text-center" v-if="response.googleId">Si</td>
                <td class="text-center" v-else>No</td>
                <td class="text-center" v-if="response.estado && me.isSuperAdmin"><a  @click='bloqueaUsuario(response.id)' class="btn btn-danger btn-sm"><i class="fa fa-lock"></a></td>
                <td class="text-center" v-else if="!response.estado && me.isSuperAdmin"><a  @click='desbloqueaUsuario(response.id)' class="btn btn-default btn-sm"><i class="fa fa-unlock"></a></td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
